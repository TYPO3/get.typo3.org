{
  "type": "project",
  "name": "t3o/gettypo3org",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Susanne Moog",
      "email": "susanne.moog@typo3.com"
    },
    {
      "name": "Benni Mack",
      "email": "benni@typo3.org"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "ext-sqlite3": "*",
    "erusev/parsedown": "^1.7",
    "guzzlehttp/guzzle": "^6.3",
    "jms/serializer-bundle": "3.0",
    "nelmio/api-doc-bundle": "^3.1",
    "nelmio/cors-bundle": "^1.5",
    "nelmio/security-bundle": "^2.5",
    "psr/simple-cache": "^1.0",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/asset": "^4.0",
    "symfony/cache": "^4.0",
    "symfony/console": "^4.0",
    "symfony/event-dispatcher": "^4.0",
    "symfony/expression-language": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/form": "^4.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/maker-bundle": "^1.2",
    "symfony/monolog-bundle": "^3.2",
    "symfony/orm-pack": "^1.0",
    "symfony/security-bundle": "^4.0",
    "symfony/twig-bundle": "^4.0",
    "symfony/validator": "^4.0",
    "symfony/yaml": "^4.0",
    "twig/extensions": "^1.5"
  },
  "require-dev": {
    "overtrue/phplint": "^1.1",
    "phpunit/phpunit": "^7.0",
    "rector/rector": "^0.5.10",
    "roave/security-advisories": "dev-master",
    "symfony/dotenv": "^4.0",
    "symfony/profiler-pack": "^1.0"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "test:php:lint": [
      "phplint"
    ],
    "test:php:unit": [
      "phpunit -c phpunit.xml.dist"
    ],
    "test": [
      "@test:php:lint",
      "@test:php:unit"
    ],
    "auto-scripts": {
      "cache:clear --no-optional-warmers": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C8T4G8QYJT0T9Q9SWDHVW8AC",
      "allow-contrib": false
    }
  }
}
