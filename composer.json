{
    "name": "t3o/gettypo3org",
    "description": "TYPO3 Download Site 'get.typo3.org'",
    "type": "project",
    "keywords": [
        "TYPO3 CMS",
        "download"
    ],
    "homepage": "https://get.typo3.org",
    "readme": "README.md",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Benni Mack",
            "email": "benni@typo3.org"
        },
        {
            "name": "Simon Gilli",
            "email": "typo3@gilbertsoft.org"
        },
        {
            "name": "Susanne Moog",
            "email": "susanne.moog@typo3.com"
        }
    ],
    "support": {
        "chat": "https://typo3.slack.com/archives/C779C18G5",
        "issues": "https://github.com/TYPO3/get.typo3.org/issues",
        "source": "https://github.com/TYPO3/get.typo3.org"
    },
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-sqlite3": "*",
        "erusev/parsedown": "^1.7",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "^3.0",
        "knplabs/knp-menu-bundle": "^3.0",
        "nelmio/api-doc-bundle": "^3.1",
        "nelmio/cors-bundle": "^2.0",
        "nelmio/security-bundle": "^2.5",
        "psr/simple-cache": "^1.0",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "5.0.*",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/expression-language": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.0.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/http-client": "5.0.*",
        "symfony/intl": "5.0.*",
        "symfony/mailer": "5.0.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.0.*",
        "symfony/orm-pack": "*",
        "symfony/process": "5.0.*",
        "symfony/security-bundle": "5.0.*",
        "symfony/serializer-pack": "*",
        "symfony/string": "5.0.*",
        "symfony/translation": "5.0.*",
        "symfony/twig-pack": "*",
        "symfony/validator": "5.0.*",
        "symfony/web-link": "5.0.*",
        "symfony/yaml": "5.0.*",
        "t3g/symfony-template-bundle": "^2.1",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "overtrue/phplint": "^1.2",
        "rector/rector": "^0.6.14",
        "roave/security-advisories": "dev-master",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/panther": "^0.7.1",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.3"
        },
        "preferred-install": {
            "t3g/*": "source",
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "test:php:cgl": [
            "php-cs-fixer --diff -v fix --format=junit > var/log/php-cs-fixer.xml"
        ],
        "test:php:lint": [
            "phplint"
        ],
        "test:php:unit": [
            "phpunit -c phpunit.xml.dist --log-junit var/log/phpunit-unit.xml --testsuite \"Unit Test Suite\""
        ],
        "test:php:functional": [
            "phpunit -c phpunit.xml.dist --log-junit var/log/phpunit-functional.xml --testsuite \"Functional Test Suite\""
        ],
        "test": [
            "@test:php:cgl",
            "@test:php:lint",
            "@test:php:unit",
            "@test:php:functional"
        ],
        "auto-scripts": {
            "cache:clear --no-optional-warmers": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "id": "01C8T4G8QYJT0T9Q9SWDHVW8AC",
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
