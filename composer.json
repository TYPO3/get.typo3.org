{
    "name": "t3o/get.typo3.org",
    "type": "project",
    "description": "TYPO3 Download Site 'get.typo3.org'",
    "keywords": [
        "TYPO3 CMS",
        "download"
    ],
    "homepage": "https://get.typo3.org",
    "readme": "README.md",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Benni Mack",
            "email": "benni@typo3.org"
        },
        {
            "name": "Simon Gilli",
            "email": "typo3@gilbertsoft.org"
        },
        {
            "name": "Susanne Moog",
            "email": "susanne.moog@typo3.com"
        }
    ],
    "require": {
        "php": "^7.4.16",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-sqlite3": "*",
        "erusev/parsedown": "^1.7",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "^3.0",
        "knplabs/knp-menu-bundle": "^3.0",
        "nelmio/api-doc-bundle": "^3.1",
        "nelmio/cors-bundle": "^2.0",
        "nelmio/security-bundle": "^2.5",
        "psr/simple-cache": "^1.0",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.1.*",
        "symfony/framework-bundle": "5.1.*",
        "symfony/http-client": "5.1.*",
        "symfony/intl": "5.1.*",
        "symfony/mailer": "5.1.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.1.*",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/string": "5.1.*",
        "symfony/translation": "5.1.*",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "5.1.*",
        "symfony/web-link": "5.1.*",
        "symfony/yaml": "5.1.*",
        "t3g/symfony-template-bundle": "^3"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "fakerphp/faker": "dev-main",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/panther": "^0.7.1",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.16"
        },
        "preferred-install": {
            "t3g/*": "source",
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "target-directory": "tools"
        },
        "symfony": {
            "allow-contrib": false,
            "id": "01C8T4G8QYJT0T9Q9SWDHVW8AC",
            "require": "5.1.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "repositories": {
        "local": {
            "type": "path",
            "url": "packages/*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts",
            "@bin:install"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@bin:update"
        ],
        "auto-scripts": {
            "cache:clear --no-optional-warmers": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "bin": "echo 'bin not installed.'",
        "bin:install": "@composer bin all install --ansi",
        "bin:update": "@composer bin all update --ansi",
        "ci": [
            "@ci:static"
        ],
        "ci:composer": [
            "@ci:composer:normalize",
            "@ci:composer:validate"
        ],
        "ci:composer:normalize": "composer-normalize.phar --dry-run",
        "ci:composer:validate": "@composer validate",
        "ci:dynamic": [
            "@ci:tests"
        ],
        "ci:json:lint": "jsonlint -q composer.json",
        "ci:php": [
            "@ci:php:copypaste",
            "@ci:php:lint",
            "@ci:php:sniff",
            "@ci:php:stan",
            "@ci:php:cs-fixer"
        ],
        "ci:php:copypaste": "phpcpd src tests",
        "ci:php:cs-fixer": "php-cs-fixer fix -v --dry-run --diff --diff-format=udiff",
        "ci:php:lint": "phplint --no-progress",
        "ci:php:rector": "rector process --dry-run --no-progress-bar --no-diffs",
        "ci:php:sniff": "phpcs src",
        "ci:php:stan": "phpstan analyse --no-progress",
        "ci:static": [
            "@ci:composer",
            "@ci:json:lint",
            "@ci:php",
            "@ci:yaml:lint"
        ],
        "ci:tests": [
            "@ci:tests:php"
        ],
        "ci:tests:php": [
            "@ci:tests:php:unit",
            "@ci:tests:php:functional"
        ],
        "ci:tests:php:functional": "phpunit -c phpunit.xml.dist --log-junit var/log/phpunit-functional.xml --testsuite \"Functional Test Suite\"",
        "ci:tests:php:unit": "phpunit -c phpunit.xml.dist --log-junit var/log/phpunit-unit.xml --testsuite \"Unit Test Suite\"",
        "ci:yaml:lint": "yaml-lint .ddev config",
        "fix": [
            "@fix:composer",
            "@fix:php"
        ],
        "fix:composer": [
            "@fix:composer:normalize"
        ],
        "fix:composer:normalize": "composer-normalize.phar",
        "fix:php": [
            "@fix:php:cs-fixer"
        ],
        "fix:php:cs-fixer": "php-cs-fixer fix",
        "fix:php:rector": "rector process --no-diffs",
        "fix:php:rector:process": "rector process --no-diffs",
        "fix:php:sniff": "phpcbf src",
        "tools:setup:install": "@composer bin all install --ansi",
        "tools:setup:update": "@composer bin all update --ansi"
    },
    "scripts-descriptions": {
        "tools:setup:install": "Install development tools.",
        "tools:setup:update": "Update development tools."
    },
    "support": {
        "issues": "https://github.com/TYPO3/get.typo3.org/issues",
        "chat": "https://typo3.slack.com/archives/C779C18G5",
        "source": "https://github.com/TYPO3/get.typo3.org"
    }
}
