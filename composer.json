{
  "type": "project",
  "name": "t3o/gettypo3org",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Susanne Moog",
      "email": "susanne.moog@typo3.com"
    },
    {
      "name": "Benni Mack",
      "email": "benni@typo3.org"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-sqlite3": "*",
    "erusev/parsedown": "^1.7",
    "guzzlehttp/guzzle": "^6.3",
    "jms/serializer-bundle": "3.0",
    "knplabs/knp-menu-bundle": "^2.3",
    "nelmio/api-doc-bundle": "^3.1",
    "nelmio/cors-bundle": "^1.5",
    "nelmio/security-bundle": "^2.5",
    "psr/simple-cache": "^1.0",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/asset": "^4.4",
    "symfony/cache": "^4.4",
    "symfony/console": "^4.4",
    "symfony/event-dispatcher": "^4.4",
    "symfony/expression-language": "^4.4",
    "symfony/flex": "^1.0",
    "symfony/form": "^4.4",
    "symfony/framework-bundle": "^4.4",
    "symfony/monolog-bundle": "^3.2",
    "symfony/orm-pack": "^1.0",
    "symfony/security-bundle": "^4.4",
    "symfony/twig-bundle": "^4.4",
    "symfony/validator": "^4.4",
    "symfony/yaml": "^4.4",
    "t3g/symfony-template-bundle": "^2.0",
    "twig/extensions": "^1.5"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "friendsofphp/php-cs-fixer": "^2.16",
    "fzaninotto/faker": "^1.9",
    "overtrue/phplint": "^1.2",
    "rector/rector": "^0.6.14",
    "roave/security-advisories": "dev-master",
    "symfony/browser-kit": "^4.4",
    "symfony/css-selector": "^4.4",
    "symfony/dotenv": "^4.4",
    "symfony/maker-bundle": "^1.14",
    "symfony/panther": "^0.6.0",
    "symfony/phpunit-bridge": "^4.4",
    "symfony/profiler-pack": "^1.0",
    "symfony/test-pack": "^1.0"
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": {
      "t3g/*": "source",
      "*": "dist"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "test:php:cgl": [
      "php-cs-fixer --diff -v fix --format=junit > var/log/php-cs-fixer.xml"
    ],
    "test:php:lint": [
      "phplint"
    ],
    "test:php:unit": [
      "simple-phpunit -c phpunit.xml.dist --log-junit var/log/phpunit-unit.xml --testsuite \"Unit Test Suite\""
    ],
    "test:php:functional": [
      "simple-phpunit -c phpunit.xml.dist --log-junit var/log/phpunit-functional.xml --testsuite \"Functional Test Suite\""
    ],
    "test": [
      "@test:php:cgl",
      "@test:php:lint",
      "@test:php:unit",
      "@test:php:functional"
    ],
    "auto-scripts": {
      "cache:clear --no-optional-warmers": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C8T4G8QYJT0T9Q9SWDHVW8AC",
      "allow-contrib": false
    }
  }
}
