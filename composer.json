{
    "name": "t3o/get.typo3.org",
    "description": "TYPO3 Download Site 'get.typo3.org'",
    "license": "GPL-2.0-or-later",
    "type": "project",
    "keywords": [
        "TYPO3 CMS",
        "download"
    ],
    "readme": "README.md",
    "authors": [
        {
            "name": "Benni Mack",
            "email": "benni@typo3.org"
        },
        {
            "name": "Simon Gilli",
            "email": "simon.gilli@typo3.org"
        },
        {
            "name": "Susanne Moog",
            "email": "susanne.moog@typo3.com"
        }
    ],
    "homepage": "https://get.typo3.org",
    "support": {
        "issues": "https://github.com/TYPO3/get.typo3.org/issues",
        "chat": "https://typo3.slack.com/archives/C779C18G5",
        "source": "https://github.com/TYPO3/get.typo3.org"
    },
    "require": {
        "php": ">=7.4.25",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-sqlite3": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^2.2.2",
        "doctrine/orm": "^2.9",
        "erusev/parsedown": "^1.7",
        "guzzlehttp/guzzle": "^7.4",
        "jms/serializer-bundle": "^3.0",
        "knplabs/knp-menu-bundle": "^3.0",
        "nelmio/api-doc-bundle": "^3.1",
        "nelmio/cors-bundle": "^2.0",
        "nelmio/security-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.2",
        "psr/simple-cache": "^1.0",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/mime": "5.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.4.*",
        "symfony/process": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/string": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/web-link": "5.4.*",
        "symfony/yaml": "5.4.*",
        "t3g/symfony-template-bundle": "^3.3.6 || dev-bugfix/reintroduce-form-elements-class-support@dev",
        "twig/extra-bundle": "^2.12 || ^3.0",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4.1",
        "doctrine/doctrine-fixtures-bundle": "^3.4.0",
        "ergebnis/composer-normalize": "^2.18",
        "fakerphp/faker": "^1.15.0",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/panther": "^1.1.1",
        "symfony/phpunit-bridge": "5.4.*",
        "symfony/stopwatch": "5.4.*",
        "symfony/var-dumper": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "repositories": {
        "local": {
            "type": "path",
            "url": "packages/*",
            "exclude": [
                "t3g/symfony-template-bundle"
            ]
        },
        "template-bundle": {
            "type": "vcs",
            "url": "https://github.com/gilbertsoft/symfony-template-bundle"
        }
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1.4"
        },
        "preferred-install": {
            "t3g/*": "source",
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "target-directory": "tools"
        },
        "symfony": {
            "allow-contrib": false,
            "id": "01C8T4G8QYJT0T9Q9SWDHVW8AC",
            "require": "5.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts",
            "@tools:setup:install"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@tools:setup:update"
        ],
        "auto-scripts": {
            "cache:clear --no-optional-warmers": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "bin": "echo 'bin not installed.'",
        "ci": [
            "@ci:static"
        ],
        "ci:composer": [
            "@ci:composer:validate",
            "@ci:composer:normalize"
        ],
        "ci:composer:normalize": "@composer normalize --dry-run",
        "ci:composer:validate": "@composer validate",
        "ci:dynamic": [
            "@ci:tests"
        ],
        "ci:json:lint": "jsonlint -q composer.json",
        "ci:php": [
            "@ci:php:copypaste",
            "@ci:php:lint",
            "@ci:php:sniff",
            "@ci:php:stan",
            "@ci:php:cs-fixer"
        ],
        "ci:php:copypaste": "phpcpd src tests",
        "ci:php:cs-fixer": "php-cs-fixer fix -v --dry-run --diff",
        "ci:php:lint": "phplint --no-progress",
        "ci:php:rector": "rector process --dry-run --no-progress-bar --no-diffs",
        "ci:php:sniff": "phpcs --standard=PSR12 src tests",
        "ci:php:stan": "phpstan analyse --no-progress",
        "ci:static": [
            "@ci:composer",
            "@ci:json:lint",
            "@ci:php",
            "@ci:yaml:lint"
        ],
        "ci:tests": [
            "@ci:tests:php"
        ],
        "ci:tests:php": [
            "@ci:tests:php:unit",
            "@ci:tests:php:functional"
        ],
        "ci:tests:php:functional": "simple-phpunit -c phpunit.xml.dist --log-junit var/log/phpunit-functional.xml --testsuite \"Functional Test Suite\"",
        "ci:tests:php:unit": "simple-phpunit -c phpunit.xml.dist --log-junit var/log/phpunit-unit.xml --testsuite \"Unit Test Suite\"",
        "ci:yaml:lint": "yaml-lint .ddev config",
        "fix": [
            "@fix:composer",
            "@fix:php"
        ],
        "fix:composer": [
            "@fix:composer:normalize"
        ],
        "fix:composer:normalize": "@composer normalize",
        "fix:php": [
            "@fix:php:rector",
            "@fix:php:cs-fixer"
        ],
        "fix:php:cs-fixer": "php-cs-fixer fix",
        "fix:php:rector": "rector process --no-diffs",
        "fix:php:sniff": "phpcbf src",
        "tools:php:rector": "rector process --dry-run",
        "tools:setup:clean": "rm -r tools/**/composer.lock tools/**/vendor",
        "tools:setup:install": "@composer bin all install --ansi",
        "tools:setup:update": "@composer bin all update --ansi"
    },
    "scripts-descriptions": {
        "tools:setup:install": "Install development tools.",
        "tools:setup:update": "Update development tools."
    }
}
