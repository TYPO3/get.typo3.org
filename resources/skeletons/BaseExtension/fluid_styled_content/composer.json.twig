{
    "name": "{{ package.vendorNameAlternative }}/{{ package.packageNameAlternative }}",
    "type": "typo3-cms-extension",
    "description": "{{ package.description}}",
    "homepage": "{{ package.author.homepage }}",
    "license": ["GPL-2.0-or-later"],
    "keywords": ["TYPO3 CMS"],
    "require": {
        "typo3/cms-core": "{% if package.typo3version < 9000000 %}^8.7{% elseif package.typo3version < 10000000 %}^9.5{% elseif package.typo3version < 11000000 %}^10.4{% else %}^11.5{% endif %}",
        "typo3/cms-rte-ckeditor": "{% if package.typo3version < 9000000 %}^8.7{% elseif package.typo3version < 10000000 %}^9.5{% elseif package.typo3version < 11000000 %}^10.4{% else %}^11.5{% endif %}",
        "typo3/cms-fluid-styled-content": "{% if package.typo3version < 9000000 %}^8.7{% elseif package.typo3version < 10000000 %}^9.5{% elseif package.typo3version < 11000000 %}^10.4{% else %}^11.5{% endif %}"
    },
    "autoload": {
        "psr-4": {
            "{{ package.vendorName }}\\{{ package.packageName }}\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "{{ package.extensionKey }}"
        }
    }
}
